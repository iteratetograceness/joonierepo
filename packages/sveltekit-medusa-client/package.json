{
	"name": "sveltekit-medusa-client",
	"version": "1.10.1",
	"description": "A client library for communicating with a Medusa ecommerce backend in SvelteKit",
	"repository": {
		"type": "git",
		"url": "https://github.com/pevey/sveltekit-medusa-client"
	},
	"homepage": "https://pevey.com/sveltekit-medusa-client",
	"author": "Lacey Pevey",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"medusa",
		"ecommerce",
		"client",
		"headless commerce",
		"medusa-plugin"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./medusa": {
			"types": "./dist/medusa.d.ts",
			"svelte": "./dist/medusa.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.0",
		"@sveltejs/package": "^2.0.2",
		"@types/cookie": "^0.5.1",
		"cookie": "^0.5.0",
		"publint": "^0.1.12",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.9"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
